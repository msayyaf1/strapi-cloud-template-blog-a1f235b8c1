{
  "kind": "collectionType",
  "collectionName": "filming_booking_forms",
  "info": {
    "singularName": "filming-booking-form",
    "pluralName": "filming-booking-forms",
    "displayName": "Filming Booking Form",
    "description": "Filming location booking form submissions"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "companyName": {
      "type": "string"
    },
    "contactNumber": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "date",
      "required": true
    },
    "time": {
      "type": "time",
      "required": true
    },
    "locations": {
      "type": "text",
      "required": true
    },
    "submittedAt": {
      "type": "datetime"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "reviewed",
        "contacted",
        "approved",
        "rejected"
      ],
      "default": "pending"
    }
  }
}